 <h1>Social <%= title %> </h1>

 <section id="feed-post">     
        <h4>Posts</h4>
        <% if(locals.user){ %> 
        <form action="/posts/create" id="new-post-form" method="post" >
        <textarea name="content" cols="30" rows="2" placeholder="Type here...." required></textarea>
        <input type="submit" value="Post">
        </form>
        <% } %>
        <div>
                <ul id="post-list-container">
                        <% for(post of posts){ %> 
                        <%- include('_post') -%>

                        <%}%>
                </ul>
        </div>
</section>